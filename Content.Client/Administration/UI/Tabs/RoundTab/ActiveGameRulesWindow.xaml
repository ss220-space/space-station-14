<DefaultWindow
    xmlns="https://spacestation14.io"
    xmlns:cc="clr-namespace:Content.Client.Administration.UI.CustomControls"
    Title="{Loc administration-ui-round-tab-active-gamerules}" MinSize="425 272">
    <BoxContainer Orientation="Vertical">
        <cc:GameRulesListControl Name="GameRulesList" VerticalExpand="True" />
        <BoxContainer Orientation="Horizontal">
            <Button Name="SubmitEndGameruleButton" Text="{Loc administration-ui-active-gamerules-end}" Disabled="True"/>
            <Button Name="SubmitClearGamerulesButton" Text="{Loc administration-ui-active-gamerules-clear}" Disabled="False"/>
        </BoxContainer>
    </BoxContainer>
</DefaultWindow>
